import scl: 'http://www.iec.ch/61850/2003/SCL'

package scl

context DAI

    inv DAI_nothing
    :
    true


   --  a DAI must refer an existing DA in DataTypeTemplate section
     inv DAI_RefersToAbstractDataAttribute
      (  'ERROR;'
          + 'OCL/SemanticConstraints/DAI_RefersToAbstractDataAttribute;'
          + self.filename + ';'
          + self.lineNumber.toString() + ';'
          + 'DAI (name=' + self.name.toString() + ') does not refer an existing DA in DataTypeTemplates section'
	 )
       :
       self.RefersToAbstractDataAttribute <> null


endpackage

